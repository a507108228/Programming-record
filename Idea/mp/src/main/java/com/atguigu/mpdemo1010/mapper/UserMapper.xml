<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.atguigu.mpdemo1010.mapper.UserMapper">

    <!--List<User> selectiveByDeleted(@Param("deleted") Boolean deleted);-->

    <select id="selectiveByDeleted" resultType="com.atguigu.mpdemo1010.entity.User">
        select *
        from mybatis_plus.user
        where deleted = #{deleted};
    </select>

</mapper>